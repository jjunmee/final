<configuration>
	<!-- <typeAliases>
		<typeAlias type="" alias=""/>
	</typeAliases> -->
	<typeAliases>
		<typeAlias type="com.jhta.netflix.content.vo.ContentVo" alias="content"/>
		<typeAlias type="com.jhta.netflix.category.vo.CategoryVo" alias="category"/>
		<typeAlias type="com.jhta.netflix.category.vo.CateGenreVo" alias="categenre"/>
		<typeAlias type="com.jhta.netflix.user.vo.UserVo" alias="user"/>
		<typeAlias type="com.jhta.netflix.user.vo.CertificationVo" alias="certifi"/>
