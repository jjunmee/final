		<typeAlias type="com.jhta.netflix.qna.vo.QnaVo" alias="qna"/>		
	</typeAliases>
	
	<mappers>
		<mapper resource="mapper/UserMapper.xml"/>
		<mapper resource="mapper/ContentMapper.xml"/>
		<mapper resource="mapper/CategoryMapper.xml"/>
		<mapper resource="mapper/GenreMapper.xml"/>
		<mapper resource="mapper/StaffMapper.xml"/>
		<mapper resource="mapper/SurveyMapper.xml"/>
		<mapper resource="mapper/SeriesMapper.xml"/>
		<mapper resource="mapper/Content_genreMapper.xml"/>
		<mapper resource="mapper/Content_staffMapper.xml"/>
		<mapper resource="mapper/QnaMapper.xml"/>
