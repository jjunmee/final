<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.CategoryMapper">
	<insert id="insert" parameterType="com.jhta.netflix.category.vo.CategoryVo">
		INSERT INTO category (category_name) VALUES (#{category_name})
	</insert>
	<select id="list" resultType="com.jhta.netflix.category.vo.CategoryVo">
		select * from category
	</select>
</mapper>
  
